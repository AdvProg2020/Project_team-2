<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="view.Controllers$AdminAccountManagingMenuController">
    <center>
        <TabPane maxWidth="-Infinity">
            <Tab closable="false" text="Admins">
                <VBox alignment="CENTER">
                    <TableView fx:id="admins" focusTraversable="false" stylesheets="@../css/TableView.css"
                               VBox.vgrow="ALWAYS">
                        <columnResizePolicy>
                            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                        </columnResizePolicy>
                        <columns>
                            <TableColumn fx:id="adminIdCOL" editable="false" prefWidth="200.0" resizable="false"
                                         text="ID"/>
                            <TableColumn fx:id="adminUsernameCOL" editable="false" prefWidth="200.0" resizable="false"
                                         text="Username"/>
                            <TableColumn fx:id="adminFullNameCOL" editable="false" prefWidth="200.0" resizable="false"
                                         text="Full Name"/>
                            <TableColumn fx:id="adminPhoneCOL" editable="false" prefWidth="200.0" resizable="false"
                                         text="Phone"/>
                            <TableColumn fx:id="adminDetailsCOL" editable="false" prefWidth="100.0" resizable="false"
                                         sortable="false" text="Details"/>
                            <TableColumn fx:id="adminRemoveCOL" editable="false" prefWidth="100.0" resizable="false"
                                         sortable="false" text="Remove"/>
                        </columns>
                        <VBox.margin>
                            <Insets/>
                        </VBox.margin>
                        <padding>
                            <Insets bottom="5.0"/>
                        </padding>
                    </TableView>
                    <HBox alignment="CENTER_RIGHT" spacing="10.0">
                        <padding>
                            <Insets bottom="20.0" left="10.0" right="10.0" top="10.0"/>
                        </padding>
                        <Button id="register-admin-button" fx:id="registerAdminBTN" mnemonicParsing="false"
                                styleClass="action-button" text="Register Admin"/>
                    </HBox>
                </VBox>
            </Tab>
            <Tab closable="false" text="Sellers">
                <VBox alignment="CENTER">
                    <TableView fx:id="sellers" focusTraversable="false" stylesheets="@../css/TableView.css"
                               VBox.vgrow="ALWAYS">
                        <columnResizePolicy>
                            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                        </columnResizePolicy>
                        <columns>
                            <TableColumn fx:id="sellerIdCOL" prefWidth="200.0" resizable="false" text="ID"/>
                            <TableColumn fx:id="sellerUsernameCOL" prefWidth="200.0" resizable="false" text="Username"/>
                            <TableColumn fx:id="sellerFullNameCOL" prefWidth="200.0" resizable="false"
                                         text="Full Name"/>
                            <TableColumn fx:id="sellerPhoneCOL" prefWidth="200.0" resizable="false" text="Phone"/>
                            <TableColumn fx:id="sellerDetailsCOL" prefWidth="100.0" resizable="false" text="Details"/>
                            <TableColumn fx:id="sellerRemoveCOL" prefWidth="100.0" resizable="false" text="Remove"/>
                        </columns>
                        <VBox.margin>
                            <Insets/>
                        </VBox.margin>
                        <padding>
                            <Insets bottom="5.0"/>
                        </padding>
                    </TableView>
                    <HBox alignment="CENTER_RIGHT" spacing="10.0">
                        <padding>
                            <Insets bottom="20.0" left="10.0" right="10.0" top="10.0"/>
                        </padding>
                        <Button id="register-admin-button" disable="true" focusTraversable="false"
                                mnemonicParsing="false" styleClass="action-button" text="Register Seller"
                                visible="false"/>
                    </HBox>
                </VBox>
            </Tab>
            <Tab closable="false" text="Customers">
                <VBox alignment="CENTER">
                    <TableView fx:id="customers" focusTraversable="false" stylesheets="@../css/TableView.css"
                               VBox.vgrow="ALWAYS">
                        <columns>
                            <TableColumn fx:id="customerIdCOL" editable="false" prefWidth="200.0" resizable="false"
                                         text="ID"/>
                            <TableColumn fx:id="customerUsernameCOL" editable="false" prefWidth="200.0"
                                         resizable="false" text="Username"/>
                            <TableColumn fx:id="customerFullNameCOL" editable="false" prefWidth="200.0"
                                         resizable="false" text="Full Name"/>
                            <TableColumn fx:id="customerPhoneCOL" editable="false" prefWidth="200.0" resizable="false"
                                         text="Phone"/>
                            <TableColumn fx:id="customerDetailsCOL" editable="false" prefWidth="100.0" resizable="false"
                                         sortable="false" text="Details"/>
                            <TableColumn fx:id="customerRemoveCOL" editable="false" prefWidth="100.0" resizable="false"
                                         sortable="false" text="Remove"/>
                        </columns>
                        <VBox.margin>
                            <Insets/>
                        </VBox.margin>
                        <padding>
                            <Insets bottom="5.0"/>
                        </padding>
                    </TableView>
                    <HBox alignment="CENTER_RIGHT" spacing="10.0">
                        <padding>
                            <Insets bottom="20.0" left="10.0" right="10.0" top="10.0"/>
                        </padding>
                        <Button id="register-admin-button" disable="true" focusTraversable="false"
                                mnemonicParsing="false" styleClass="action-button" text="Register Customer"
                                visible="false"/>
                    </HBox>
                </VBox>
            </Tab>
            <Tab closable="false" text="Supporters">
                <VBox alignment="CENTER">
                    <TableView fx:id="supporters" focusTraversable="false" stylesheets="@../css/TableView.css"
                               VBox.vgrow="ALWAYS">
                        <columnResizePolicy>
                            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                        </columnResizePolicy>
                        <columns>
                            <TableColumn fx:id="supporterIdCOL" editable="false" prefWidth="200.0" resizable="false"
                                         text="ID"/>
                            <TableColumn fx:id="supporterUsernameCOL" editable="false" prefWidth="200.0"
                                         resizable="false"
                                         text="Username"/>
                            <TableColumn fx:id="supporterFullNameCOL" editable="false" prefWidth="200.0"
                                         resizable="false"
                                         text="Full Name"/>
                            <TableColumn fx:id="supporterPhoneCOL" editable="false" prefWidth="200.0" resizable="false"
                                         text="Phone"/>
                            <TableColumn fx:id="supporterDetailsCOL" editable="false" prefWidth="100.0"
                                         resizable="false"
                                         sortable="false" text="Details"/>
                            <TableColumn fx:id="supporterRemoveCOL" editable="false" prefWidth="100.0" resizable="false"
                                         sortable="false" text="Remove"/>
                        </columns>
                        <VBox.margin>
                            <Insets/>
                        </VBox.margin>
                        <padding>
                            <Insets bottom="5.0"/>
                        </padding>
                    </TableView>
                    <HBox alignment="CENTER_RIGHT" spacing="10.0">
                        <padding>
                            <Insets bottom="20.0" left="10.0" right="10.0" top="10.0"/>
                        </padding>
                        <Button id="register-supporter-button" fx:id="registerSupporterBTN" mnemonicParsing="false"
                                styleClass="action-button" text="Register Supporter"/>
                    </HBox>
                </VBox>
            </Tab>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
            <stylesheets>
                <URL value="@../css/TabPane.css"/>
                <URL value="@../css/ManagingMenu.css"/>
            </stylesheets>
        </TabPane>
    </center>
</BorderPane>
