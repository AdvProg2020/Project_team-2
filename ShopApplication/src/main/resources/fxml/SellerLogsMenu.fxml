<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" styleClass="bg-pane" stylesheets="@../css/ManagingMenu.css"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="view.GUI.Controllers$SellerLogsMenu">
    <center>
        <VBox alignment="CENTER" maxWidth="-Infinity" BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
            <TableView fx:id="products" focusTraversable="false" stylesheets="@../css/TableView.css"
                       VBox.vgrow="ALWAYS">
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                </columnResizePolicy>
                <columns>
                    <TableColumn fx:id="dateCol" prefWidth="150.0" resizable="false" text="Date"/>
                    <TableColumn fx:id="customerCOL" prefWidth="200.0" resizable="false" text="Customer"/>
                    <TableColumn fx:id="receivedMoneyCOL" prefWidth="150.0" resizable="false" text="Received Money"/>
                    <TableColumn fx:id="saleAmountCOL" prefWidth="150.0" resizable="false" text="Total Sale"/>
                    <TableColumn fx:id="shippingStatusCOL" prefWidth="150.0" resizable="false" text="Shipping Status"/>
                    <TableColumn fx:id="detailsCOL" prefWidth="100.0" resizable="false" text="Details"/>
                </columns>
                <VBox.margin>
                    <Insets/>
                </VBox.margin>
                <padding>
                    <Insets bottom="5.0"/>
                </padding>
            </TableView>
            <HBox alignment="CENTER" spacing="10.0">
                <padding>
                    <Insets bottom="20.0" left="10.0" right="10.0" top="10.0"/>
                </padding>
                <Label id="message-label" fx:id="errorLBL" text="Possible error"/>
                <Region HBox.hgrow="ALWAYS"/>
                <Button mnemonicParsing="false" styleClass="action-button" visible="false"/>
            </HBox>
        </VBox>
    </center>
</BorderPane>
