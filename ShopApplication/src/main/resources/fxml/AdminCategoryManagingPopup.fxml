<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="500.0" prefWidth="750.0"
            stylesheets="@../css/ManagingPopup.css" xmlns="http://javafx.com/javafx/11.0.1">
    <padding>
        <Insets bottom="30.0" left="30.0" right="30.0" top="40.0"/>
    </padding>
    <left>
        <VBox BorderPane.alignment="CENTER">
            <TabPane maxWidth="-Infinity" stylesheets="@../css/TabPane.css">
                <Tab closable="false" text="Pending Requests">
                    <TableView fx:id="subCategories" maxWidth="-Infinity" stylesheets="@../css/TableView.css">
                        <columns>
                            <TableColumn fx:id="idCOL" editable="false" maxWidth="150.0" minWidth="150.0"
                                         prefWidth="150.0" text="Product ID"/>
                            <TableColumn fx:id="productRemoveCOL" editable="false" maxWidth="100.0" minWidth="100.0"
                                         prefWidth="100.0" resizable="false" sortable="false" text="Remove"/>
                        </columns>
                        <columnResizePolicy>
                            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                        </columnResizePolicy>
                    </TableView>
                </Tab>
                <Tab closable="false" text="Archive">
                    <TableView fx:id="customers" maxWidth="-Infinity" stylesheets="@../css/TableView.css">
                        <columns>
                            <TableColumn fx:id="usernameCOL" maxWidth="150.0" minWidth="150.0" prefWidth="150.0"
                                         text="Customer"/>
                            <TableColumn fx:id="countCOL" editable="false" maxWidth="150.0" minWidth="150.0"
                                         prefWidth="150.0" text="Count"/>
                            <TableColumn fx:id="removeCOL" editable="false" maxWidth="100.0" minWidth="100.0"
                                         prefWidth="100.0" resizable="false" sortable="false" text="Remove"/>
                        </columns>
                        <columnResizePolicy>
                            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                        </columnResizePolicy>
                    </TableView>
                </Tab>
                <padding>
                    <Insets top="20.0"/>
                </padding>
            </TabPane>
        </VBox>
    </left>
    <center>
        <VBox alignment="CENTER" BorderPane.alignment="CENTER">
            <Region VBox.vgrow="ALWAYS"/>
            <GridPane VBox.vgrow="ALWAYS">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="101.60001220703126" minWidth="10.0"
                                       prefWidth="84.80003662109375"/>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="174.20001220703125" minWidth="10.0"
                                       prefWidth="124.79996337890627"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <Label layoutX="10.0" layoutY="47.0" styleClass="property-label" text="ID:"/>
                <Label fx:id="idLBL" layoutX="110.0" layoutY="47.0" styleClass="value-label" text="Label"
                       GridPane.columnIndex="1"/>
                <Label styleClass="property-label" text="Code:" GridPane.rowIndex="1"/>
                <Label styleClass="property-label" text="Percentage:" GridPane.rowIndex="2"/>
                <Label styleClass="property-label" text="Max:" GridPane.rowIndex="3"/>
                <Label styleClass="property-label" text="Start Date:" GridPane.rowIndex="4"/>
                <Label styleClass="property-label" text="End Date:" GridPane.rowIndex="5"/>
                <HBox alignment="CENTER_LEFT" spacing="10.0" styleClass="property-box" GridPane.columnIndex="1"
                      GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="10.0" top="10.0"/>
                    </GridPane.margin>
                    <padding>
                        <Insets right="5.0"/>
                    </padding>
                    <TextField fx:id="codeField"/>
                </HBox>
                <HBox alignment="CENTER_LEFT" spacing="10.0" styleClass="property-box" GridPane.columnIndex="1"
                      GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets bottom="10.0" top="10.0"/>
                    </GridPane.margin>
                    <padding>
                        <Insets right="5.0"/>
                    </padding>
                    <TextField fx:id="percentageField" prefWidth="130.0" promptText="0 - 100"/>
                    <Label alignment="CENTER" text="\%" textAlignment="CENTER">
                        <HBox.margin>
                            <Insets/>
                        </HBox.margin>
                    </Label>
                </HBox>
                <HBox alignment="CENTER_LEFT" spacing="10.0" styleClass="property-box" GridPane.columnIndex="1"
                      GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets bottom="10.0" top="10.0"/>
                    </GridPane.margin>
                    <padding>
                        <Insets right="5.0"/>
                    </padding>
                    <TextField fx:id="maxField" prefWidth="130.0"/>
                    <Label alignment="CENTER" text="\$" textAlignment="CENTER"/>
                </HBox>
                <HBox alignment="CENTER_LEFT" styleClass="property-box" GridPane.columnIndex="1" GridPane.rowIndex="4">
                    <GridPane.margin>
                        <Insets bottom="10.0" top="10.0"/>
                    </GridPane.margin>
                    <DatePicker fx:id="startDate" editable="false" promptText="select date..."
                                stylesheets="@../css/DatePicker.css"/>
                </HBox>
                <HBox alignment="CENTER_LEFT" styleClass="property-box" GridPane.columnIndex="1" GridPane.rowIndex="5">
                    <GridPane.margin>
                        <Insets bottom="10.0" top="10.0"/>
                    </GridPane.margin>
                    <DatePicker fx:id="endDate" editable="false" promptText="select date..."
                                stylesheets="@../css/DatePicker.css"/>
                </HBox>
            </GridPane>
            <Region layoutX="10.0" layoutY="10.0" VBox.vgrow="ALWAYS"/>
            <StackPane>
                <VBox.margin>
                    <Insets/>
                </VBox.margin>
                <Button fx:id="addBTN" mnemonicParsing="false" styleClass="confirm-button" text="Add"/>
                <Button fx:id="editBTN" layoutX="89.0" layoutY="10.0" mnemonicParsing="false"
                        styleClass="confirm-button" text="Save"/>
            </StackPane>
            <BorderPane.margin>
                <Insets left="30.0"/>
            </BorderPane.margin>
            <padding>
                <Insets bottom="10.0"/>
            </padding>
        </VBox>
    </center>
</BorderPane>
